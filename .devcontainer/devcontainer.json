{
    "name": "Clubber",
    "postCreateCommand": "echo \"alias clubber='/workspaces/Real-Estate/clubber/clubber'\" >> ~/.bashrc && source ~/.bashrc",
    "postAttachCommand": {
        "server": "clubber setup && clubber build && clubber serve"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "mathematic.vscode-pdf"
            ]
        }
    },
    "portsAttributes": {
        "4000": {
            "label": "Application",
            "onAutoForward": "openBrowser"
        }
    }
}